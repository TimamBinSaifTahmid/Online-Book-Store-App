<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="signUpPane" prefHeight="554.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="register.SignUpController">
   <children>
      <AnchorPane layoutX="128.0" layoutY="105.0" prefHeight="383.0" prefWidth="344.0">
         <children>
            <TextField fx:id="name" layoutX="19.0" layoutY="55.0" prefHeight="25.0" prefWidth="308.0" promptText="Name" />
            <TextField fx:id="email" layoutX="19.0" layoutY="92.0" prefHeight="25.0" prefWidth="308.0" promptText="Email" />
            <TextField fx:id="phone" layoutX="19.0" layoutY="126.0" prefHeight="25.0" prefWidth="308.0" promptText="Phone" />
            <TextField fx:id="address" layoutX="19.0" layoutY="167.0" prefHeight="25.0" prefWidth="308.0" promptText="Address" />
            <TextField fx:id="dob" layoutX="19.0" layoutY="205.0" prefHeight="25.0" prefWidth="308.0" promptText="Date Of Birth (yyyy-mm-dd)" />
            <TextField fx:id="password" layoutX="19.0" layoutY="249.0" prefHeight="25.0" prefWidth="308.0" promptText="Password" />
            <Button layoutX="141.0" layoutY="336.0" mnemonicParsing="false" onAction="#onSignUpButtonClick" text="Sign Up" />
         </children>
      </AnchorPane>
      <Label layoutX="165.0" layoutY="60.0" text="Create New Account">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
   </children>
</AnchorPane>
