<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="dashboardPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dashboard.DashboardController">
   <children>
      <AnchorPane fx:id="bar_pane" prefHeight="42.0" prefWidth="634.0" style="-fx-background-color: #087654;">
         <children>
            <Button layoutX="527.0" layoutY="9.0" mnemonicParsing="false" onAction="#onProfileButtonClick" text="Profile" />
            <Button layoutX="359.0" layoutY="9.0" mnemonicParsing="false" onAction="#onSearchButtonClick" text="Search" />
            <Button layoutX="422.0" layoutY="9.0" mnemonicParsing="false" onAction="#onPendingRatingButtonClick" text="Pending Rating" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="90.0" layoutY="56.0" prefHeight="539.0" prefWidth="404.0">
         <children>
            <Label fx:id="listName_label" layoutX="14.0" layoutY="24.0" text="Books">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <ListView fx:id="listView" layoutX="14.0" layoutY="56.0" prefHeight="484.0" prefWidth="379.0" />
            <AnchorPane fx:id="customerPane" layoutX="190.0" layoutY="13.0" prefHeight="42.0" prefWidth="200.0">
               <children>
                  <Button layoutX="35.0" layoutY="9.0" mnemonicParsing="false" onAction="#onAllBooksButtonClick" text="All Books" />
                  <Button layoutX="126.0" layoutY="9.0" mnemonicParsing="false" onAction="#onWishlistButtonClick" text="WishList" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="optionPane" layoutX="486.0" layoutY="98.0" prefHeight="200.0" prefWidth="148.0">
         <children>
            <Label layoutX="23.0" layoutY="42.0" text="Select to view" />
            <Button layoutX="14.0" layoutY="118.0" mnemonicParsing="false" onAction="#onDueOrderClick" text="Due Order list" />
            <Button layoutX="14.0" layoutY="75.0" mnemonicParsing="false" onAction="#onAllOrdersButtonClick" prefHeight="25.0" prefWidth="90.0" text="All Orders" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
